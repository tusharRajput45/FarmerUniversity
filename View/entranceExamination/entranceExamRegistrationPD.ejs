<%- include('../Layout/bootstrap') %>
<%- include('./Layout/entranceExamRegistrationPD') %>
<html>
    <head>
        <title>Entrance Exam Registration</title>
    </head> 
    <body class="">
       
        <div class="container" id="container">
            <form action="/entranceExamination/PostEntranceExamRegistrationPD"  id="SubmitForm">
        <div><h3>Student Personal Details</h3></div>
        <div class="row">
            <div class="col">
                <label for=""> Name</label>
                <input type="text" name="Name" id="Name" class="input" value="<%= Result.Name %>" placeholder="Name" readonly>
            </div>
            <div class="col">
                <label for="">Father's Name</label>
                <input type="text" name="FatherName" id="FatherName" class="input" placeholder="Father's Name" required>
            </div>
        </div>
        <div class="row">
            <div class="col">
                <label for=""> Mother's Name</label>
                <input type="text" name="MotherName" id="MotherName" class="input" placeholder="Mother's Name" required>
            </div>
            <div class="col">
                <label for="">Gender</label>
                <select name="Gender" id="Gender" class="input" required>
                    <option value="">Select Gender</option>
                    <option value="MALE">MALE</option>
                    <option value="FEMALE">FEMALE</option>
                    <option value="OTHER">OTHER</option>
                </select>
            </div>
        </div>
        <div class="row">
            <div class="col">
                <label for=""> Categories</label>
                <select name="Categories" id="Categories" class="input" required>
                    <option value="">Select Categories</option>
                    <option value="General">General</option>
                    <option value="OBC">OBC</option>
                    <option value="SC/ST">SC/ST</option>
                    <option value="Other Category">Other Category</option>

                </select>
            </div>
            <div class="col">
                <label for="">Caste</label>
                <select name="Caste" id="Caste" class="input" required>
                    <option value="">Select Caste</option>
                    <option value="Hindu">Hindu</option>
                    <option value="Muslim">Muslim</option>
                    <!-- <option value="Cristian">Cristian</option> -->
                    <option value="OTHER">OTHER</option>
                </select>
            </div>
        </div>
        <div class="row">
            <div class="col">
                <label for="">Email</label>
                <input type="email" name="Email" id="Email" class="input" value="<%= Result.Email %>" placeholder="Email" readonly>
            </div>
            <div class="col">
                <label for="">Mobile</label>
                <input type="tel" name="Mobile" id="Mobile" class="input" value="<%= Result.Mobile %>" placeholder="Mobile"minlength="10" maxlength="10"  readonly>
            </div>
        </div>
        <div class="row">
            <div class="col">
                <label for="">AadharNO</label>
                <input type="tel" name="AadharNO" id="AadharNO"minlength="12" maxlength="12" class="input" placeholder="Aadhar NO"required>
            </div>
            <div class="col">
                    <label for="">DOB</label>
                    <input type="tel" name="DOB" id="DOB"minlength="12" maxlength="12" class="input" placeholder="DD/MM/YYYY" required>
            </div>
        </div>
        <div class="row">
            <div class="col">
              <label>Country</label>
              <select name="Country" class="countries input" id="countryId" required>
            <option value="">Select Country</option>
        </select>
            </div>
            <div class="col">
                <label>State</label>
                <select name="State" class="states input" id="stateId" class="input" required>
              <option value="">Select State</option>
          </select>
      </div>
        </div>
        <div class="row">
          <div class="col">
             <label>City</label>        
             <select name="City" class="cities input" id="cityId" class="input" required>
                <option value="">Select City</option>
             </select>
          </div>
          <div class="col">
            <label for="">Address</label>
            <input type="text" name="Address" id="Address" class="input" placeholder="Address" required>
        </div>
        </div>
        <br><br>
        <div style="margin-top: 2%;"><h3>Student Education Details</h3></div>
        <div class="row">
            <div class="col">
                <label for="">High School RollNO</label>
                <input type="tel" name="HighSchoolRollNO" id="HighSchoolRollNO" class="input" placeholder="High School RollNO"  required>
            </div>
            <div class="col">
                <label for="">High School Passed Year</label>
                <select name="HighSchoolYear" id="HighSchoolYear" class="input" required>
                    <option value="">Select Year</option>
                </select>
            </div>
        </div>
        <div class="row">
            <div class="col">
                <label for="">InterMediate RollNO</label>
                <input type="tel" name="InterMediateRollNO" id="InterMediateRollNO" class="input" placeholder="InterMediate RollNO"required>
            </div>
            <div class="col">
                <label for="">InterMediate Passed Year</label>
                <select name="InterMediateYear" class="input" id="InterMediateYear" required>
                    <option value="">Select Year</option>
                </select>
            </div>
        </div><br><br>
        <div><h3>Last Guration Course</h3></div>
        <div class="row">
            <div class="col">
                <label for="">Guration Course</label>
                 <select name="GurationCourse" class="input" id="GurationCourse" >
                    <option value="">Select Guration Course</option>
                    <option value="BCA">BCA</option>
                    <option value="BA">BA</option>
                    <option value="BAA">BAA</option>

                 </select>      
            </div>
            <div class="col">
                <label for="">Guration University Name</label>
                <select name="GurationUniversityName" class="input" id="GurationUniversityName" >
                    <option value="">Select University Name</option>
                </select>
            </div>
        </div>
        <div class="row">
            <div class="col">
                <label for="">Guration RollNO</label>
                <input type="tel" name="GurationRollNO" id="GurationRollNO" class="input" placeholder="Guration RollNO"required>
            </div>
            <div class="col">
                <label for="">Guration Passed Year</label>
                <select name="GurationYear" id="GurationYear"class="input" required>
                    <option value="">Select Year</option>
                </select>
            </div>
        </div>
       
<div class="container">
<table class="table table-bordered mt-5">
<thead>
<tr>
<th scope="col">Class/Course</th>
<th scope="col">Roll NO</th>
<th scope="col">Marks Obtained</th>
<th scope="col">Maximum Marks </th>
<th scope="col">Percentage </th>
<th scope="col">Board/University </th>
<th scope="col">Action</th>
</tr>
</thead>
<tbody id="dynamicadd">
<tr>
<td><input type="text" name="ClassORCourse[]" id="" class="form-control" placeholder="Class/Course" required></td>
<td><input type="text" name="RollNO[]" id="" class="form-control"placeholder="Roll NO" required></td>
<td><input type="text" name="MarksObtained[]" id="MarksObtained" class="form-control MarksObtained" placeholder="Marks Obtained" onchange="calc(this)" required></td>
<td><input type="text" name="MaxMarks[]" id="MaxMarks" class="form-control MaxMarks" placeholder="Maximum Marks" onchange="calc(this)" required></td>
<td><input type="text" name="Parcentage[]" id="Parcentage" class="form-control Parcentage" placeholder="Percentage" readonly required></td>
<td><input type="text" name="BoardORUniversity[]" id="" class="form-control" placeholder="Board/University" required></td>
<td><button type="button" id="add" class="btn btn-warning">+</button></td>
</tr>
</tbody>
</table>
        <br><br><input type="submit" value="Submit" id="SubmitButton" class="btn btn-success">
        <input type="button" value="Close" class="btn btn-Danger ml-4" onclick="Refresh()">

            </form>
        </div>
    </body>
</html>
<script>
    function Refresh(){
        location.reload();
    }
</script>
